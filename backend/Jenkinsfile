pipeline {
    agent any

    stages {
        stage('Hello Jenkins from Backend') {
            steps {
                // Change working directory to 'backend'
                dir('backend') {
                    // This will print "Hello Jenkins from Backend"
                    echo 'Hello Jenkins from the backend folder'

                  
                }
            }
        }
    }

    post {
        always {
            echo 'Pipeline finished.'
        }
    }
}
